{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "safari >= 7"]
      }
    }],
    "stage-0",
    "react"
  ],
  "plugins": [
    "add-module-exports",
    ["transform-runtime", { "polyfill": false }],
    "transform-decorators-legacy",
    "transform-async-to-generator",
    ["import", {"libraryName": "antd", "style": true }],
    ["module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "src": "./src"
        }
      }
    ]
  ]
}
