{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "safari >= 7"]
      }
    }],
    "react",
    "stage-0"
  ],
  "plugins": [
    "add-module-exports",
    ["transform-runtime", { "polyfill": false }],
    "transform-decorators-legacy",
    "transform-async-to-generator",
    ["import", {"libraryName": "antd", "style": true }],
    ["resolver", {"resolveDirs": ["src"]}]
  ]
}